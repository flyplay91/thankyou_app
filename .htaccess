RewriteEngine on 
RewriteCond %{HTTP_HOST} ^(www.)?dashboard-ohana.com$ 
RewriteCond %{REQUEST_URI} !^/public/
RewriteCond %{REQUEST_FILENAME} !-f 
RewriteCond %{REQUEST_FILENAME} !-d 
RewriteRule ^(.*)$ /public/$1 

RewriteCond %{HTTP_HOST} ^(www.)?dashboard-ohana.com$ 
RewriteRule ^(/)?$ public/index.php